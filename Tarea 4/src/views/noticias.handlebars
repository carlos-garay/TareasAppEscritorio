
{{#each noticias}}
    <div>
        <div class ="section">
            <a href = {{this.url}} target = "_blank"><h3>{{this.title}}</h3></a>
        </div>
        <br>
       
        <img src = {{this.urlToImage}}  alt="Imagen de la noticia" width="40%" height="20%" class = "centrar" >
        <br>
        <label>{{this.description}}</label>
        <br>
        <label>Autor: {{this.author}}</label>

    </div>
{{/each}}

